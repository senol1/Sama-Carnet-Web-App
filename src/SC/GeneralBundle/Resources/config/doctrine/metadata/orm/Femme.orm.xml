<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Femme" table="femme">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="prenom" type="string" column="prenom" length="45" nullable="true"/>
    <field name="nom" type="string" column="nom" length="45" nullable="true"/>
    <field name="email" type="string" column="email" length="45" nullable="true"/>
    <field name="adresse" type="string" column="adresse" length="245" nullable="true"/>
    <field name="ville" type="string" column="ville" length="45" nullable="true"/>
    <field name="pays" type="string" column="Pays" length="45" nullable="true"/>
    <field name="telephone" type="integer" column="telephone" nullable="true"/>
    <field name="age" type="integer" column="age" nullable="true"/>
    <field name="nombreenfants" type="integer" column="nombreEnfants" nullable="true"/>
    <field name="etatgrossesse" type="integer" column="etatGrossesse" nullable="true"/>
    <field name="status" type="boolean" column="status" nullable="true"/>
  </entity>
</doctrine-mapping>
